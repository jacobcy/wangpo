                <!-- Page Heading -->
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            用户资料库
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i>  <a href="index.html">Dashboard</a>
                            </li>
                            <li class="active">
                                <i class="fa fa-table"></i> 用户资料库
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- /.row -->

                <div class="row">
                    <div class="col-lg-6">
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th>微博内部ID</th>
                                        <th>微博帐号</th>
                                        <th>出生日期</th>
                                        <th>性别</th>
                                        <th>身高</th>
                                        <th>所在地</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (i = 0; i < weibousers.length; i++){ %>
                                    <tr>
                                        <td><%= weibousers[i].innerId %></td>
                                        <td><%= weibousers[i].userName %></td>
                                        <td><%= weibousers[i].formattedBirthday %></td>
                                        <td><%= weibousers[i].gender %></td>
                                        <td><%= weibousers[i].height %></td>
                                        <td><%= weibousers[i].userLocation %></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>


